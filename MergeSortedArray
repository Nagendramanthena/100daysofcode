class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int k=0;
        for(int i=0;i<n;i++){
                nums1[m+k] = nums2[i];
                k++;
            }
            
        Arrays.sort(nums1);
     
    }
}


// day1 5th sum
//Squares of sorted arrays


class Solution {
    public int[] sortedSquares(int[] nums) {
        for(int i=0;i<nums.length;i++){
            nums[i] = nums[i]*nums[i];
        }
        
        Arrays.sort(nums);
        int b[] = nums.clone();
            
    return b;
        
    }
    
}
