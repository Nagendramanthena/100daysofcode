// max_wealth leetcode

class Solution {
    public int maximumWealth(int[][] accounts) {
        int max_wealth = 0;
        for(int i=0;i<accounts.length;i++){
            int cu=0;
            for(int j=0;j<accounts[i].length;j++){
                cu = cu+accounts[i][j];
                if(cu>max_wealth){
                    max_wealth = cu;
                }
            }
      
        }
      return max_wealth;  
    }
}
