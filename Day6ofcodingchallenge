// My calender
public class MyCalendar {
    List<int[]> calendar;

    MyCalendar() {
        calendar = new ArrayList();
    }

    public boolean book(int start, int end) {
        for (int[] k: calendar) {
            if (k[0] < end && start < k[1]) return false;
        }
        calendar.add(new int[]{start, end});
        return true;
    }
}


//codechef
/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner scan = new Scanner(System.in);
		int T = scan.nextInt();
		for(int i=0;i<T;i++){
		    int c = scan.nextInt();
		    if(c>20)
		    System.out.println("HOT");
		    else{
		        System.out.println("COLD");
		    }
		}
	}
}


//Insurance
import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int i=0;i<T;i++){
		    int a = sc.nextInt();
		    int b = sc.nextInt();
		    if(a>=b){
		        System.out.println(b);
		    }
		    else{
		        System.out.println(a);
		    }
		}
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int i=0;i<T;i++){
		    int a = sc.nextInt();
		    int b = sc.nextInt();
		    System.out.println(4*a+b);
		}
	}
}


